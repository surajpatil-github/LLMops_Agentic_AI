{
  "family": "llmopstdlive",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::854173464596:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "llmops-live-container",
      "image": "854173464596.dkr.ecr.eu-north-1.amazonaws.com/llmopsrepolive:latest",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "llmops-live-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        { "name": "ENV", "value": "production" }
      ],
      "secrets": [
        {
          "name": "apikeyliveclass",
          "valueFrom": "arn:aws:secretsmanager:eu-north-1:854173464596:secret:apikeyliveclass-TY4wwt"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/llmopstdlive",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
